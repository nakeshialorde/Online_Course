$(document).ready(function(){   
 var question = ["<b><sup>5</sup>&frasl;<sub>15</sub> with denominator 45.</b>"];
 
  var answer =["1) 8/15, 2) 5/45, 3) 15/45","1)"];

  var fw=["15/45"];
 var scr=0;
function generateQuiz(question, answer, i){
  document.getElementById("txt").value="";      
  $("#result").html("");
       $("#qDisplay").html(question[i]);
       $("#aDisplay").html(answer[i]);
     //var a =document.getElementById("txt").value;
  //$("#summa").html("a");
  $("#Next").click(function(){         checkAnswer(document.getElementById("txt").value,i);      });
  
  }
  
  
function checkAnswer(a, i){
   
    console.log(i, a, fw[i]);
   //$("#summa").html(a); 
   //$("#summa").html(fw[i]); 
    if(a == fw[i]){
      $("#result").html("Correct");
       scr++;
      }
    else{
      $("#result").html("Wrong");
      scr=scr;
    }
   
  }
  
  function moveToNextQuestion(i){
    if(i < question.length-1){
      setTimeout(function(){generateQuiz(question, answer, i+1)}, 1000);
       //$("#summa").html(a); 
  // document.getElementById("txt").value="";  
    }
    else{
      end();
    }
  }
    
  function end(){
    $("#whole").html("Great job!");
    $("#play").html("Refresh your browser to play again");
    $("#mar").html("Your score is " + scr);
  }
    
    
  generateQuiz(question, answer, 0);
});

